{
  "version": 1,
  "projects": {
    "mixcore-portal": {
      "projectType": "application",
      "root": "apps/mixcore-portal",
      "sourceRoot": "apps/mixcore-portal/src",
      "prefix": "mixcore",
      "architect": {
        "build:prod": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/mixcore-portal-1",
            "index": "apps/mixcore-portal/src/index.html",
            "main": "apps/mixcore-portal/src/main.ts",
            "polyfills": "apps/mixcore-portal/src/polyfills.ts",
            "tsConfig": "apps/mixcore-portal/tsconfig.app.json",
            "assets": [
              "apps/mixcore-portal/src/favicon.ico",
              "apps/mixcore-portal/src/assets"
            ],
            "styles": [
              "apps/mixcore-portal/src/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "apps/mixcore-portal/src/environments/environment.ts",
                "with": "apps/mixcore-portal/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../mix.core/src/Mix.Cms.Web/wwwroot/angular-io",
            "index": "apps/mixcore-portal/src/index.html",
            "main": "apps/mixcore-portal/src/main.ts",
            "polyfills": "apps/mixcore-portal/src/polyfills.ts",
            "tsConfig": "apps/mixcore-portal/tsconfig.app.json",
            "assets": [
              "apps/mixcore-portal/src/favicon.ico",
              "apps/mixcore-portal/src/assets"
            ],
            "styles": ["./node_modules/@angular/material/prebuilt-themes/indigo-pink.css", "apps/mixcore-portal/src/styles.scss"],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "apps/mixcore-portal/src/environments/environment.ts",
                "with": "apps/mixcore-portal/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mixcore-portal:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mixcore-portal:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mixcore-portal:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/mixcore-portal/src/**/*.ts",
              "apps/mixcore-portal/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/mixcore-portal"],
          "options": {
            "jestConfig": "apps/mixcore-portal/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mixcore-portal-e2e": {
      "root": "apps/mixcore-portal-e2e",
      "sourceRoot": "apps/mixcore-portal-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/mixcore-portal-e2e/cypress.json",
            "tsConfig": "apps/mixcore-portal-e2e/tsconfig.e2e.json",
            "devServerTarget": "mixcore-portal:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mixcore-portal:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/mixcore-portal-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "mixcore-portal"
}
